<?xml version="1.0" encoding="UTF-8"?>
<JMF xmlns="http://www.CIP4.org/JDFSchema_1_1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="JMFRootMessage">
  <!--QueryQueueStatus provides status for all jobs that exist in the system-->
<Query ID="TRMJP" Type="QueueStatus" xsi:type="QueryQueueStatus" >
  <!-- QueueEntryDetails JobPhase will show the progress per job in amount and percentage-->
  <QueueFilter QueueEntryDetails="JobPhase"/>
</Query>
</JMF>